{
  "training": {
    "task": "segmentation",
    "transfer_learning": true,
    "epochs": 20,
    "lr_policy": "step",
    "lr_decay_iters": 250,
    "preloadData": true,
    "freeze": false,
    "gradually_unfreeze": false
  },
  "data": {
    "root_dir": "/root/autodl-tmp/datasets/segmentation",
    "with_notumour": true,
    "train_batch_size": 24,
    "test_batch_size": 24,
    "seed": 42
  },
  "model": {
    "model_name": "CMUNeXt",
